play: 'flex', \n    marginTop: '6%',\n}\n\nfunction Signup () {\n    return (\n        <div>\n            <div id=\"signup-form\" style={formStyle}>\n                <SignupForm />\n            </div>\n            \n        </div>\n    ); \n}\n\nexport default Signup; ","/home/shicklus/code/git/COSC-4P02-R8Scholar/r8scholar-frontend/src/components/assets/Foot.js",[],"/home/shicklus/code/git/COSC-4P02-R8Scholar/r8scholar-frontend/src/components/assets/Navigation.js",[],"/home/shicklus/code/git/COSC-4P02-R8Scholar/r8scholar-frontend/src/components/assets/ReviewItem.js",["79","80","81"],"import React, { Component } from 'react';\nimport  {Link} from 'react-router-dom'; \nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport {Card, Accordion, Button, ListGroup, ListGroupItem} from 'react-bootstrap'; \n\n\nconst noCommentMessage = \"No comments yet!\";\n\nconst cardStyle = {\n    width: '60rem', \n    padding: '20px', \n    marginTop: '17px',\n}\n\nexport default class ReviewItem extends Component {\n\n    //props is going to consist of the review item passed by the course \n    constructor(props){\n        super(props); \n    }\n\n    //the JSX that is rendered when this file is imported as a component \n    render() {\n        return (\n            <div className=\"App\">\n                {/* container (card )which includes a title section + rating and a content section + button to see comments */}\n                    <Card style={cardStyle}>\n                        <Accordion>\n                            <Card.Body>\n                                <Card.Title>{this.props.review.title}</Card.Title>\n                                <Card.Subtitle className=\"mb-2 text-muted\">{this.props.review.user + \" - \" + this.props.review.rating + \"/5\"} </Card.Subtitle>\n                                <Card.Text>\n                                    {this.props.review.content}\n                                </Card.Text>\n                                <Accordion.Toggle